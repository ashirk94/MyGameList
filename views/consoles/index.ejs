<h2>Search Consoles</h2>
<form action="/consoles" method="GET">
    <input type="text" name="name" value="<%= searchOptions.name %>">
    <button type="submit">Search</button>
</form>
<br>
<% consoles.forEach(console => { %>
<div class="console-list"><%= console.name %></div>
<a class="btn btn-primary" href="/consoles/<%= console.id %>">View</a>
<a class="btn btn-cancel" href="/consoles/<%= console.id %>/edit">Edit</a>
<% }) %>
<br>