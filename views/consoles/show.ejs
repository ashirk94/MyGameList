<h2><%= console.name %></h2>
<a class="btn btn-primary" href="/consoles/<%= console.id %>/edit">Edit</a>

<% if (gamesByConsole.length > 0) { %>
<h2>Games On <%= console.name %></h2>
<div>
  <% gamesByConsole.forEach(game => { %>
    <a href="/games/<%= game.id %>">
      <img height="250" width="200" src="https://s3-us-west-2.amazonaws.com/mygamelistimages98/<%= game.imageName %>">
    </a>
  <% }) %>
  <% } else { %> <%- include('../partials/deleteForm', { url:
    `${console.id}` }) %> <% } %>
</div>
