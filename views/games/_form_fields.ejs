<label>Title</label>
<input type="text" name="title" value="<%= console.title%>">
<label>Console</label>
<select name="console">
<% consoles.forEach(console => { %>
    <% if (console.id === game.console) { %>
        <option label="<%= console.name %>" value="<%= console.id %>">
    <% } else { %>
        <option label="<%= console.name %>" value="<%= console.id %>">
    <% } %>
<% }) %>
</select>
<label>Release Date</label>
<input type="date" name="releaseDate"
    value="<%= game.releaseDate == null ? '' : game.releaseDate.toISOString().split('T') %>">