<h2>Search Games</h2>
<form action="/games" method="GET">
    <div class="game-list">
    <label>Title</label>
    <input type="text" name="title" value="<%= searchOptions.title %>">
    </div>
    <div class="game-list">
        <label>Released After</label>
        <input type="date" name="releasedAfter" value="<%= searchOptions.releasedAfter %>">
    </div>
    <div class="game-list">
        <label>Released Before</label>
        <input type="date" name="releasedBefore" value="<%= searchOptions.releasedBefore %>">
    </div>
    <button type="submit">Search</button>
</form>
<br>
<div>
    <% games.forEach(game => { %>
        <a href="/games/<%= game.id %>">
            <img height="250" width="200" src="<%= game.imagePath %>">
        </a>
    <% }) %>
</div>